<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.zykj.cloud.mapper.IndexPointThMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.zykj.cloud.entity.IndexPointTh">
        <id column="id" property="id" />
        <result column="classID" property="classID" />
        <result column="method" property="method" />
        <result column="th" property="th" />
        <result column="pf" property="pf" />
        <result column="indexPointMsg" property="indexPointMsg" />
        <result column="score" property="score" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, classID, method, th, pf, indexPointMsg, score
    </sql>

</mapper>
