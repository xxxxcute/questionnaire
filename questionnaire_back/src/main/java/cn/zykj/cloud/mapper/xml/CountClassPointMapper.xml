<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.zykj.cloud.mapper.CountClassPointMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.zykj.cloud.entity.CountClassPoint">
        <id column="id" property="id" />
        <result column="school" property="school" />
        <result column="year" property="year" />
        <result column="major" property="major" />
        <result column="courseName" property="courseName" />
        <result column="className" property="className" />
        <result column="indexPoint" property="indexPoint" />
        <result column="zdcd" property="zdcd" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, school, year, major, courseName, className, indexPoint, zdcd
    </sql>

</mapper>
